<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Tg_bot_GUI_crossplatform"
             x:Class="Tg_bot_GUI_crossplatform.App"
             RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
             
    <Application.Resources>
        <FontFamily x:Key="Tech">avares://Tg_bot_GUI_crossplatform/Assets/Fonts/ShareTechMono-Regular.ttf#Share Tech Mono</FontFamily>
    </Application.Resources>
</Application>